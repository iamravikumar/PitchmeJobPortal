                    </div>
                </div><!-- navbar-left -->
                <!-- nav-right -->
                <div class="nav-right">
                    <ul class="sign-in">
                                         
                        @Html.Partial("_LoginPartial")
                   
                    </ul><!-- sign-in -->

                    <a href="~/job/post" class="btn"  style="margin-right:7px">Post Your Job</a>
                </div>
